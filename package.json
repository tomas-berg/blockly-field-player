{
  "name": "blockly-field-player",
  "version": "1.0.1",
  "description": "A Blockly audio player field",
  "scripts": {
    "audit:fix": "blockly-scripts auditFix",
    "build": "NODE_OPTIONS=--openssl-legacy-provider blockly-scripts build",
    "clean": "blockly-scripts clean",
    "lint": "blockly-scripts lint",
    "predeploy": "blockly-scripts predeploy",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "blockly-scripts start",
    "test": "blockly-scripts test"
  },
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "unpkg": "./dist/index.js",
  "author": "",
  "keywords": [
    "blockly",
    "blockly-plugin",
    "blockly-field",
    "player"
  ],
  "homepage": "https://github.com/tomas-berg/blockly-field-player#readme",
  "bugs": "https://github.com/tomas-berg/blockly-field-player/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/tomas-berg/blockly-field-player.git"
  },
  "license": "Apache-2.0",
  "directories": {
    "dist": "dist",
    "src": "src"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@blockly/dev-scripts": "^1.2.23",
    "@blockly/dev-tools": "^2.6.1",
    "blockly": "^5.20210325.0"
  },
  "peerDependencies": {
    "blockly": "^5.20210325.0"
  },
  "publishConfig": {},
  "eslintConfig": {
    "extends": "@blockly/eslint-config"
  },
  "engines": {
    "node": ">=8.17.0"
  },
  "browserslist": [
    "defaults",
    "IE 11",
    "IE_Mob 11"
  ],
  "dependencies": {
    "wavesurfer.js": "^6.2.0"
  }
}
